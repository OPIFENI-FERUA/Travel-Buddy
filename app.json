{
  "expo": {
    "name": "Buddy",
    "slug": "Buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#000000",
    "backgroundColor": "#656655",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/splash-screen.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://buddy.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
